package doctorSurgeon;

public class Hospital {

	public static void main(String[] args) {

		Doctor doc = new Doctor();
		doc.name = "Jogn";
		doc.speciality = "Doctor";
		doc.setName("John");
		doc.setSpeciality("Doctor");
		System.out.println(doc.getInfo());

		Surgeon sur = new Surgeon();
		sur.setName("Mike");
		sur.setSpeciality("Surgeon");
		System.out.println(sur.getInfo());

	}

}
