package nizi;

public class NiziExample3 {
	
	public static void passByReferrence(String niza[]) {
		
		niza[0] = "Changed";
		
	}
	
	public static void main(String[] args) {
		
		String nizaa[] = {"Apple", "Mango", "Orange"};
		
		System.out.println("Pred da ja povikame funkcijata: " + nizaa[0]);
		
		passByReferrence(nizaa);
		
		System.out.println("Otkako ja povikavme funkcijata: " + niza[0]);
	}

}
