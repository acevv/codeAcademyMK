package doctorExample;

public class Hospital {

	public static void main(String[] args) {

		Doctor doc = new Doctor();
		doc.name = "John";
		doc.setSpeciality("Doctor");
		doc.getInfo();
		doc.treatPatient();

		Surgeon sur = new Surgeon();
		sur.setName("Mike");
		sur.setSpeciality("Surgeon");
		sur.getInfo();
		sur.treatPatient();

//		Problem ne dozvoluva da se konvertira od Doktor vo Surgeon
//		Doctor dok = new Surgeon();

	}

}
