package doctorSurgeon;

public class Hospital {

	public static void main(String[] args) {

		Doctor doc = new Doctor();
		doc.setName("John");
		doc.setSpeciality("Doctor");
		doc.getInfo();

		Surgeon sur = new Surgeon();
		sur.setName("Mike");
		sur.setSpeciality("Surgeon");
		sur.getInfo();
	
	}

}
